{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1 || ^8.2",
        "doctrine/dbal": "^4.2",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^3.9",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "mcamara/laravel-localization": "^1.8",
        "mkocansey/bladewind": "^2.4",
        "php-flasher/flasher-laravel": "^1.15",
        "php-flasher/flasher-toastr-laravel": "^1.15",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/image": "^3.7",
        "spatie/image-optimizer": "^1.7",
        "spatie/laravel-image-optimizer": "^1.8",
        "spatie/laravel-medialibrary": "^11.12",
        "spatie/laravel-permission": "^6.7",
        "spatie/laravel-translatable": "^6.5",
        "spatie/laravel-view-models": "^1.5",
        "yajra/laravel-datatables": "^10.0",
        "yajra/laravel-datatables-oracle": "^10.3.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "fakerphp/faker": "^1.9.1",
        "laravel/boost": "^1.0",
        "laravel/breeze": "^1.29",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.29",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Barryvdh\\DomPDF\\": "vendor/barryvdh/laravel-dompdf/src/",
            "Socialblue\\DatabaseSynchronizer\\": "Package/Socialblue/DatabaseSynchronizer/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
